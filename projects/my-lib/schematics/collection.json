{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "blank": {
      "description": "Generate file.",
      "factory": "./blank/index#generate",
      "schema": "./blank/schema.json"
    },
    "layout": {
      "description": "Generate layout code.",
      "factory": "./layout/index#modifyHtml",
      "schema": "./layout/schema.json"
    },
    "text": {
      "description": "Generate text code.",
      "factory": "./text/index#modifyHtml",
      "schema": "./text/schema.json"
    }
  }
}
