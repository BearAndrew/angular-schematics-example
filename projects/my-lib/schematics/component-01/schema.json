{
  "$schema": "http://json-schema.org/schema",
  "$id": "Component01",
  "title": "Component01",
  "description": "Generate a file of JavaScript",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "File name"
    },
    "path": {
      "type": "string",
      "format": "path",
      "description": "The path to create the service.",
      "visible": false
    },
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    }
  },
  "required": [
    "name"
  ]
}
